

<div class="row">
    @foreach (var item in Model)
    {
    <div class="col-md-4">
        <a class="nav-link text-dark" asp-area="" asp-controller=@item.controller asp-action=@item.action>
            <div class="profile-card-2">
                <img src=@item.imgUrl class="img-responsive">
                <div class="profile-name">@item.title</div>
                
            </div>
        </a>
    </div>
    }
</div>
